{
    "dockerFile": "Dockerfile",
    "extensions": [
        "akamud.vscode-theme-onelight",
        "eamodio.gitlens",
        "GitHub.github-vscode-theme",
        "github.github-vscode-theme",
        "matklad.rust-analyzer",
        "ms-vscode.theme-1337",
        "tamasfe.even-better-toml",
        "Tyriar.sort-lines",
        "usernamehw.errorlens",
        "vadimcn.vscode-lldb"
    ],
    "settings": {
        "editor.tokenColorCustomizations": {
            "[1337]": {
                "textMateRules": [
                    {
                        "scope": "comment",
                        "settings": {
                            "foreground": "#FFF"
                        }
                    }
                ]
            },
            "[Atom One Dark]": {
                "textMateRules": [
                    {
                        "scope": "comment",
                        "settings": {
                            "foreground": "#FFF"
                        }
                    }
                ]
            },
            "[Atom One Light]": {
                "textMateRules": [
                    {
                        "scope": "comment",
                        "settings": {
                            "foreground": "#000"
                        }
                    }
                ]
            },
            "[GitHub Dark Default]": {
                "textMateRules": [
                    {
                        "scope": "comment",
                        "settings": {
                            "foreground": "#FFF"
                        }
                    }
                ]
            },
            "[GitHub Light Default]": {
                "textMateRules": [
                    {
                        "scope": "comment",
                        "settings": {
                            "foreground": "#000"
                        }
                    }
                ]
            },
            "textMateRules": [
                {
                    "scope": "punctuation.definition.comment",
                    "settings": {
                        "fontStyle": "bold",
                        "foreground": "#0A44"
                    }
                },
                {
                    "scope": "comment",
                    "settings": {
                        "fontStyle": ""
                    }
                }
            ]
        },
        "workbench.colorTheme": "1337"
    }
}
